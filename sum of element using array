import java.util.*;
class ArraySum {
    public static void main(String[] args) {
        int[] arr = {5, 8, 12, 3, 7};
        System.out.println("Sum of elements: " + sumArray(arr));
    }

    public static int sumArray(int[] arr) {
        int sum = 0;
        for (int num : arr) {
            sum += num;
        }
        return sum;
    }
}
